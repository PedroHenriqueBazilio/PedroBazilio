programa {
  funcao inicio() {
    inteiro a, b, w, x, y, z
    escreva("Escreva 3 numeros: \n")
    leia(a,b)
    w = a + b
    y = a * b
    x = a - b
    z = a / b
    escreva("Valor de W e: "+w +"\n")
    escreva("Valor de x e: "+x +"\n")
    escreva("Valor de y e: "+y +"\n")
    escreva("Valor de z e: "+z +"\n")
  }
}
